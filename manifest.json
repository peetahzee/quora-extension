{
  "name": "Quome",
  "version": "0.1",
  "manifest_version": 2,

  "description": "A plain text description",

  "browser_action": {
  	"default_popup": "popup.html"
  },
  "background": {
    "scripts": ["jquery.min.js", "global.js", "background.js"]
  },
  "permissions": [
    "cookies",
    "tabs",
  	"*://www.quora.com/",
    "*://api.quora.com/"
  ]
}